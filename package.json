{
  "name": "tasks",
  "version": "1.6.11",
  "description": "Task manager and money manager web application",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "node_modules/.bin/tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "start:ng": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "start": "node_modules/.bin/tsc src/server/server.ts --lib es6 && tsc -p src/ && node --inspect src/server/server.js",
    "build:node": "node_modules/.bin/tsc src/server/server.ts --lib es6 --module commonjs --outDir dist/",
    "servedist": "lite-server -c=dist-bs-config.json",
    "bundleapp": "node_modules/.bin/webpack --config webpack.config.js",
    "launch": "npm run build && npm run bundleapp && npm run servedist",
    "launch2": "webpack-dev-server --inline --progress --port 8080"
  },
  "keywords": [
    "tasks",
    "to do",
    "to do list",
    "money",
    "money manager"
  ],
  "author": "mycomplexsoul",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.3.4",
    "@angular/compiler": "~4.3.4",
    "@angular/core": "~4.3.4",
    "@angular/forms": "~4.3.4",
    "@angular/http": "~4.3.4",
    "@angular/platform-browser": "~4.3.4",
    "@angular/platform-browser-dynamic": "~4.3.4",
    "@angular/router": "~4.3.4",
    "angular-in-memory-web-api": "~0.3.0",
    "core-js": "^2.4.1",
    "rxjs": "5.4.2",
    "mysql": "^2.12.0",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jasmine": "2.5.36",
    "@types/mysql": "^2.15.4",
    "@types/node": "^6.0.102",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "express": "^4.16.2",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "protractor": "~4.0.14",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "~2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "repository": {}
}
